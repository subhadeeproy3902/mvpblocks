      "content": "\r\n'use client';\r\n\r\nimport { Cloud, Droplets, Sun, Wind } from 'lucide-react';\r\n\r\nimport { Badge } from '@/components/ui/badge';\r\nimport {\r\n  Expandable,\r\n  ExpandableCard,\r\n  ExpandableCardContent,\r\n  ExpandableCardFooter,\r\n  ExpandableCardHeader,\r\n  ExpandableContent,\r\n  ExpandableTrigger,\r\n} from '@/components/ui/expandable';\r\n\r\nexport default function ExpandableWeatherCard() {\r\n  return (\r\n    <Expandable expandDirection=\"both\" expandBehavior=\"replace\">\r\n      <ExpandableTrigger>\r\n        <ExpandableCard\r\n          collapsedSize={{ width: 300, height: 220 }}\r\n          expandedSize={{ width: 500, height: 420 }}\r\n          hoverToExpand={false}\r\n          expandDelay={100}\r\n          collapseDelay={400}\r\n        >\r\n          <ExpandableCardHeader>\r\n            <div className=\"flex items-center justify-between\">\r\n              <div className=\"flex items-center\">\r\n                <Sun className=\"mr-2 h-8 w-8 text-yellow-400\" />\r\n                <ExpandableContent preset=\"blur-sm\" keepMounted={true}>\r\n                  <h3 className=\"text-lg font-medium\">Today&apos;s Weather</h3>\r\n                  <Badge\r\n                    variant=\"secondary\"\r\n                    className=\"bg-blue-100 text-blue-800\"\r\n                  >\r\n                    72°F\r\n                  </Badge>\r\n                </ExpandableContent>\r\n              </div>\r\n            </div>\r\n          </ExpandableCardHeader>\r\n\r\n          <ExpandableCardContent>\r\n            <div className=\"mb-4 flex items-center justify-between\">\r\n              <div>\r\n                <p className=\"text-2xl font-bold\">72°F</p>\r\n                <p className=\"text-sm text-gray-600 dark:text-gray-400\">\r\n                  Feels like 75°F\r\n                </p>\r\n              </div>\r\n              <div className=\"text-right\">\r\n                <p className=\"font-medium\">Sunny</p>\r\n                <ExpandableContent\r\n                  preset=\"blur-sm\"\r\n                  stagger\r\n                  staggerChildren={0.1}\r\n                  keepMounted={true}\r\n                  animateIn={{\r\n                    initial: { opacity: 0, y: 20, rotate: -5 },\r\n                    animate: { opacity: 1, y: 0, rotate: 0 },\r\n                    transition: { type: 'spring', stiffness: 300, damping: 20 },\r\n                  }}\r\n                >\r\n                  <p className=\"text-sm text-gray-600 dark:text-gray-400\">\r\n                    High 78° / Low 65°\r\n                  </p>\r\n                </ExpandableContent>\r\n              </div>\r\n            </div>\r\n            <ExpandableContent\r\n              preset=\"blur-sm\"\r\n              stagger\r\n              staggerChildren={0.1}\r\n              keepMounted={true}\r\n              animateIn={{\r\n                initial: { opacity: 0, y: 20, rotate: -5 },\r\n                animate: { opacity: 1, y: 0, rotate: 0 },\r\n                transition: { type: 'spring', stiffness: 300, damping: 20 },\r\n              }}\r\n            >\r\n              <div className=\"mb-4 space-y-2\">\r\n                <div className=\"flex items-center justify-between\">\r\n                  <div className=\"flex items-center\">\r\n                    <Cloud className=\"mr-2 h-5 w-5 text-gray-400\" />\r\n                    <span>Humidity</span>\r\n                  </div>\r\n                  <span>45%</span>\r\n                </div>\r\n                <div className=\"flex items-center justify-between\">\r\n                  <div className=\"flex items-center\">\r\n                    <Wind className=\"mr-2 h-5 w-5 text-gray-400\" />\r\n                    <span>Wind</span>\r\n                  </div>\r\n                  <span>8 mph</span>\r\n                </div>\r\n                <div className=\"flex items-center justify-between\">\r\n                  <div className=\"flex items-center\">\r\n                    <Droplets className=\"mr-2 h-5 w-5 text-gray-400\" />\r\n                    <span>Precipitation</span>\r\n                  </div>\r\n                  <span>0%</span>\r\n                </div>\r\n              </div>\r\n              <div className=\"space-y-2\">\r\n                <h4 className=\"font-medium\">5-Day Forecast</h4>\r\n                {[\r\n                  { day: 'Mon', temp: 70 },\r\n                  { day: 'Tue', temp: 71 },\r\n                  { day: 'Wed', temp: 72 },\r\n                  { day: 'Thu', temp: 73 },\r\n                  { day: 'Fri', temp: 74 },\r\n                ].map((forecast) => (\r\n                  <div\r\n                    key={forecast.day}\r\n                    className=\"flex items-center justify-between\"\r\n                  >\r\n                    <span>{forecast.day}</span>\r\n                    <div className=\"flex items-center\">\r\n                      <Sun className=\"mr-2 h-4 w-4 text-yellow-400\" />\r\n                      <span>{forecast.temp}°F</span>\r\n                    </div>\r\n                  </div>\r\n                ))}\r\n              </div>\r\n            </ExpandableContent>\r\n          </ExpandableCardContent>\r\n          <ExpandableCardFooter>\r\n            <p className=\"text-xs text-gray-500 dark:text-gray-400\">\r\n              Last updated: 5 minutes ago\r\n            </p>\r\n          </ExpandableCardFooter>\r\n        </ExpandableCard>\r\n      </ExpandableTrigger>\r\n    </Expandable>\r\n  );\r\n}\r\n",
